<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>requestDispatcher</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
   	<servlet-name>Calling1</servlet-name>
   	<servlet-class>requestDispatcher.Servlet1</servlet-class>
  </servlet>
  
  <servlet-mapping>
   	<servlet-name>Calling1</servlet-name>
   	<url-pattern>/first</url-pattern> 
  </servlet-mapping>
  
	<servlet>
		<servlet-name>Calling2</servlet-name>
		<servlet-class>requestDispatcher.Servlet2</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Calling2</servlet-name>
		<url-pattern>/second</url-pattern>
	</servlet-mapping>
  
</web-app>